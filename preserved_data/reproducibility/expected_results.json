  "key_findings": {
    "finding_1": {
      "name": "Text-Baseline Parity",
      "description": "Bridge achieves sender model ceiling on Banking77 (21.5% vs 22.0%)",
      "evidence": ["banking77_bridge_16tok", "banking77_llama_text"]
    },
    "finding_2": {
      "name": "Bridge >> Text-Relay",
      "description": "Bridge outperforms text-relay by 20-24pp on ALL tasks (SST-2, AG News, Banking77)",
      "evidence": ["sst2_bridge", "text_relay_sst2", "agnews_bridge", "text_relay_agnews", "banking77_bridge_16tok", "text_relay_banking77"]
    },
    "finding_3": {
      "name": "Inverse Token Scaling",
      "description": "More soft tokens = worse performance due to mode collapse",
      "evidence": ["banking77_bridge_16tok", "banking77_bridge_32tok", "banking77_bridge_64tok", "banking77_bridge_128tok"]
    },
    "finding_4": {
      "name": "16 Tokens Optimal",
      "description": "Best performance consistently at 16 soft tokens",
      "evidence": ["banking77_bridge_16tok", "passkey_16tok"]
    },
    "finding_5": {
      "name": "Text-Relay Catastrophic Failure",
      "description": "Text-relay fails completely on fine-grained classification (1.0% on 77-class Banking77)",
      "evidence": ["text_relay_banking77", "banking77_bridge_16tok"]
    }
  },
  "models": {
